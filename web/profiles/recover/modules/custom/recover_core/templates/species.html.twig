{#
/**
 * @file species.html.twig
 * Default theme implementation to present Species data.
 *
 * This template is used when viewing Species pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_species()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('species') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
